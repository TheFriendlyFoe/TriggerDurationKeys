	"developer": "TheFriendlyFoe",
	"description": "Keybinds for adjusting trigger durations.",
	"dependencies": {
		"geode.custom-keybinds": {
			"importance": "required",
			"version": "v1.1.0"
	  	}
	},
	"settings": {
        "small_duration": {
            "type": "float",
